                    nmvYoYHist: [50, 52, 54, 56, 58, 50],
                    nsiYoYHist: [60, 62, 64, 66, 68, 60],
                    nmvUnit: 'B LC',
                    nsiUnit: 'K'
                }
            };

            // Helper function to format NSI dynamically to M or K
            function formatNSI(value) {
                // Value in the data arrays represents K units (e.g., 24000 = 24M units, 150 = 150K units)
                if (value >= 1000) {
                    const valM = value / 1000;
                    // Use 1 decimal for values under 10M, 0 decimals otherwise (e.g., 2.5 M, 12 M)
                    return `${valM.toFixed(valM < 10 ? 1 : 0)} M`;
                }
                // Small values remain in K (e.g., 150 K, 0.5 K)
                return `${value.toFixed(value >= 10 ? 0 : 1)} K`;
            }
            
            // Helper function to format NMV based on site unit
            function formatNMV(value, unit) {
                if (unit === 'M USD' || unit === 'M LC') {
                    // NMV M USD/LC -> Millones
                    return `${(value / 1).toFixed(0)} M`;
                } else if (unit === 'B LC') {
                    // NMV B LC -> Convertir valor a Billones (dividido por 1000) y formatear a 1 decimal
                    return `${(value / 1000).toFixed(1)} B`;
                }
                return value;
            }
            
            // Helper function to extract values from placeholder data objects
            function extractValues(data) {
                return data.map(item => item.v);
            }

            function extractMoM(data) {
                return data.map(item => `MoM: ${item.m}`);
            }

            function extractYoY(data) {
                return data.map(item => item.y);
            }

            const chartTooltipCallback = {
                plugins: {
                    tooltip: {
                        callbacks: {
                            title: function(tooltipItems) {
                                const item = tooltipItems[0];
                                let label = item.chart.data.labels[item.dataIndex];
                                if (Array.isArray(label)) {
                                    return label.join(' ');
                                } else {
                                    return label;
                                }
                            },
                            afterLabel: function(context) {
                                const chartId = context.chart.canvas.id;
                                const dataIndex = context.dataIndex;
                                let momArray = [];

                                if (chartId === 'nmvChart') momArray = extractMoM(nmvData);
                                else if (chartId === 'nsiChart') momArray = extractMoM(nsiData);
                                else if (chartId === 'naspChart') momArray = extractMoM(naspData);
                                else if (chartId === 'dcChart') momArray = extractMoM(dcData);
                                else if (chartId === 'cvrChart') momArray = extractMoM(cvrData);
                                else if (chartId === 'agg1NmvChart') momArray = [extractMoM(agg1Nmv)[dataIndex], extractYoY(agg1Nmv)[dataIndex]];
                                else if (chartId === 'agg1NsiChart') momArray = [extractMoM(agg1Nsi)[dataIndex], extractYoY(agg1Nsi)[dataIndex]];
                                else if (chartId === 'agg1NaspChart') momArray = [extractMoM(agg1Nasp)[dataIndex], extractYoY(agg1Nasp)[dataIndex]];

                                if (chartId.includes('Chart') && !chartId.includes('agg1')) {
                                    if (dataIndex === 0) return '';
                                    return [momArray[dataIndex]];
                                } else if (chartId.includes('agg1Chart')) {
                                    return momArray;
                                }
                                return '';
                            },
                            label: function(context) {
                                const value = context.parsed.y;
                                const dataset = context.dataset;
                                const chartId = context.chart.canvas.id;
                                
                                let label = dataset.label;
                                let formattedValue = value;

                                if (dataset.type === 'line' || dataset.label.includes('%')) {
                                    formattedValue = `${value}%`;
                                } else if (chartId.includes('site')) {
                                    const siteData = context.chart.options.siteData;
                                    const unit = (dataset.label.includes('NMV')) ? siteData.nmvUnit : siteData.nsiUnit;
                                    if (unit.includes('B LC')) {
                                        formattedValue = `${value.toFixed(1)} ${unit}`;
                                    } else if (unit.includes('M')) {
                                        formattedValue = `${value.toFixed(0)} ${unit}`;
                                    } else if (unit.includes('K')) {
                                        formattedValue = formatNSI(value);
                                    }
                                } else if (dataset.label.includes('NSI')) {
                                    formattedValue = formatNSI(value);
                                } else if (dataset.label.includes('NMV')) {
                                    // NMV Total/AGG1 is M USD
                                    formattedValue = `${value.toFixed(2)} M USD`;
                                }
                                return `${label}: ${formattedValue}`;
                            }
                        }
                    },
                    datalabels: {
                        align: 'end',
                        anchor: 'end',
                        color: '#333333',
                        font: {
                            weight: 'bold',
                            size: 10
                        },
                        formatter: (value, context) => {
                            const chartId = context.chart.canvas.id;
                            
                            if (chartId.includes('Chart') && !chartId.includes('agg1')) {
                                if (context.dataIndex === context.dataset.data.length - 1) {
                                    let yoyArray = [];
                                    if (chartId === 'nmvChart') yoyArray = extractYoY(nmvData);
                                    else if (chartId === 'nsiChart') yoyArray = extractYoY(nsiData);
                                    else if (chartId === 'naspChart') yoyArray = extractYoY(naspData);
                                    else if (chartId === 'dcChart') yoyArray = extractYoY(dcData);
                                    else if (chartId === 'cvrChart') yoyArray = extractYoY(cvrData);
                                    return `YoY: ${yoyArray[context.dataIndex]}`;
                                }
                            } else if (chartId.includes('agg1Chart')) {
                                let yoyArray = [];
                                if (chartId === 'agg1NmvChart') yoyArray = extractYoY(agg1Nmv);
                                else if (chartId === 'agg1NsiChart') yoyArray = extractYoY(agg1Nsi);
                                else if (chartId === 'agg1NaspChart') yoyArray = extractYoY(agg1Nasp);
                                return `YoY: ${yoyArray[context.dataIndex]}`;
                            }
                            return null;
                        }
                    }
                }
            };

            function createLineChart(ctxId, labels, data, label, color, bgColor) {
                const ctx = document.getElementById(ctxId);
                if (!ctx) return;
                new Chart(ctx.getContext('2d'), {
                    type: 'line',
                    data: {
                        labels: labels,
                        datasets: [{
                            label: label,
                            data: extractValues(data),
                            borderColor: color,
                            backgroundColor: bgColor,
                            fill: true,
                            tension: 0.3,
                            pointRadius: 5,
                            pointHoverRadius: 7
                        }]
                    },
                    options: {
                        responsive: true,
                        maintainAspectRatio: false,
                        scales: {
                            y: {
                                beginAtZero: false,
                                ticks: {
                                    callback: function(value) {
                                        if (ctxId === 'nsiChart') return formatNSI(value);
                                        return value; 
                                    }
                                }
                            }
                        },
                        ...chartTooltipCallback
                    }
                });
            }

            function createHorizontalBarChart(ctxId, labels, data, label, color, bgColor) {
                const ctx = document.getElementById(ctxId);
                if (!ctx) return;
                new Chart(ctx.getContext('2d'), {
                    type: 'bar',
                    data: {
                        labels: labels,
                        datasets: [{
                            label: label,
                            data: data,
                            backgroundColor: bgColor,
                            borderColor: color,
                            borderWidth: 2
                        }]
                    },
                    options: {
                        indexAxis: 'y',
                        responsive: true,
                        maintainAspectRatio: false,
                        scales: {
                            x: {
                                beginAtZero: true,
                                ticks: {
                                    callback: function(value) {
                                        if (ctxId.includes('Nsi')) return formatNSI(value);
                                        return value; 
                                    }
                                }
                            }
                        },
                        ...chartTooltipCallback,
                          plugins: {
                            ...chartTooltipCallback.plugins,
                            legend: {
                                display: false
                            }
                        }
                    }
                });
            }

            function createAgg1Chart(ctxId, data, label, color, bgColor) {
                const ctx = document.getElementById(ctxId);
                if (!ctx) return;

                new Chart(ctx.getContext('2d'), {
                    type: 'bar',
                    data: {
                        labels: agg1Labels,
                        datasets: [{
                            label: label,
                            data: extractValues(data),
                            backgroundColor: color,
                            borderColor: color,
                            borderWidth: 1
                        }]
                    },
                    options: {
                        indexAxis: 'y',
                        responsive: true,
                        maintainAspectRatio: false,
                        scales: {
                            x: {
                                beginAtZero: true,
                                title: {
                                    display: false
                                },
                                ticks: {
                                    callback: function(value) {
                                        if (ctxId === 'agg1NsiChart') return formatNSI(value);
                                        return value; 
                                    }
                                }
                            },
                            y: {
                                ticks: {
                                    font: {
                                        size: 10
                                    }
                                }
                            }
                        },
                        ...chartTooltipCallback,
                        plugins: {
                            ...chartTooltipCallback.plugins,
                            legend: {
                                display: false
                            }
                        }
                    }
                });
            }
            
            function createDonutChart(ctxId, labels, data, colors) {
                const ctx = document.getElementById(ctxId);
                if (!ctx) return;
                new Chart(ctx.getContext('2d'), {
                    type: 'doughnut',
                    data: {
                        labels: labels,
                        datasets: [{
                            data: data,
                            backgroundColor: colors
                        }]
                    },
                    options: {
                        responsive: true,
                        maintainAspectRatio: false,
                        ...chartTooltipCallback,
                        plugins: {
                            ...chartTooltipCallback.plugins,
                            legend: {
                                position: 'bottom'
                            }
                        }
                    }
                });
            }

            function createSiteHistoryChart(ctxId, site, data) {
                const ctx = document.getElementById(ctxId);
                if (!ctx) return;
                
                // NMV data conversion for chart (using absolute numbers for ticks, but converting for title)
                const nmvDataScaled = data.nmv.map(v => {
                    if (data.nmvUnit.includes('B LC')) return v / 1000; // Convert Millions (LC) to Billions (LC) for chart scale
                    return v; // Already in millions (M USD/LC)
                });
                // NSI data is already in K

                new Chart(ctx.getContext('2d'), {
                    type: 'bar',
                    data: {
                        labels: kpiLabels.map(l => l.replace(' \'25', '')), // Jun, Jul, Aug...
                        datasets: [
                            {
                                type: 'bar',
                                label: `NMV (${data.nmvUnit})`,
                                data: nmvDataScaled,
                                backgroundColor: meliYellow,
                                yAxisID: 'y'
                            },
                            {
                                type: 'bar',
                                label: `NSI (K)`,
                                data: data.nsi,
                                backgroundColor: color2,
                                yAxisID: 'y' 
                            },
                            {
                                type: 'line',
                                label: 'YoY NMV (%)',
                                data: data.nmvYoYHist,
                                borderColor: meliYellow,
                                backgroundColor: meliYellow,
                                yAxisID: 'y1',
                                fill: false,
                                tension: 0.2,
                                borderWidth: 3,
                                pointRadius: 4,
                                pointBackgroundColor: '#FFF'
                            },
                            {
                                type: 'line',
                                label: 'YoY NSI (%)',
                                data: data.nsiYoYHist,
                                borderColor: color2,
                                backgroundColor: color2,
                                yAxisID: 'y1',
                                fill: false,
                                tension: 0.2,
                                borderWidth: 3,
                                pointRadius: 4,
                                pointBackgroundColor: '#FFF'
                            }
                        ]
                    },
                    options: {
                        responsive: true,
                        maintainAspectRatio: false,
                        siteData: data,
                        scales: {
                            y: {
                                type: 'linear',
                                display: true,
                                position: 'left',
                                title: {
                                    display: true,
                                    // Title now generic, showing NMV unit
                                    text: `Valor Absoluto (${data.nmvUnit.includes('B') ? 'B LC' : data.nmvUnit})`, 
                                    color: color4,
                                    font: { size: 10 }
                                },
                                ticks: {
                                    font: { size: 9 },
                                    callback: function(value) {
                                        // Formatting for MLA/MLC/MCO/MLU/MPE/MEC (B LC)
                                        if (data.nmvUnit.includes('B LC')) return value.toFixed(1) + ' B'; // Show Billions with 1 decimal
                                        // Formatting for MLB/MLM (M USD/LC)
                                        return value.toFixed(0) + ' M';
                                    }
                                }
                            },
                            y1: {
                                type: 'linear',
                                display: true,
                                position: 'right',
                                grid: {
                                    drawOnChartArea: false
                                },
                                title: {
                                    display: true,
                                    text: 'YoY (%)',
                                    color: color4,
                                    font: { size: 10 }
                                },
                                ticks: {
                                    font: { size: 9 },
                                    callback: function(value) {
                                        return value + '%';
                                    }
                                }
                            },
                            x: {
                                ticks: {
                                    font: { size: 9 }
                                }
                            }
                        },
                        plugins: {
                            legend: { 
                                display: true,
                                position: 'bottom',
                                labels: {
                                    usePointStyle: true,
                                    pointStyle: 'rect'
                                }
                            },
                            tooltip: {
                                mode: 'index',
                                intersect: false,
                                callbacks: {
                                    title: function(tooltipItems) {
                                        return tooltipItems[0].label;
                                    },
                                    label: function(context) {
                                        const value = context.parsed.y;
                                        const dataset = context.dataset;
                                        const unit = (dataset.label.includes('NMV')) ? data.nmvUnit : (dataset.label.includes('NSI') ? data.nsiUnit : '%');
                                        
                                        let formattedValue;
                                        if (dataset.type === 'line') {
                                            formattedValue = `${value}%`;
                                        } else if (unit.includes('B LC')) {
                                            formattedValue = `${value.toFixed(1)} ${unit}`;
                                        } else if (unit.includes('M')) {
                                            formattedValue = `${value.toFixed(0)} ${unit}`;
                                        } else if (unit.includes('K')) {
                                            formattedValue = formatNSI(value);
                                        }

                                        return `${dataset.label}: ${formattedValue}`;
                                    },
                                    footer: function(tooltipItems) {
                                        return [`YoY NMV: ${data.nmvYoy}`, `YoY NSI: ${data.nsiYoy}`];
                                    }
                                }
                            },
                            datalabels: {
                                formatter: () => null // Deshabilitar datalabels para no saturar
                            }
                        }
                    }
                });
            }
            
            createLineChart('nmvChart', 'NMV (USD M)', nmvData, meliYellow, meliYellowBg);
            createLineChart('nsiChart', 'NSI (K)', nsiData, color2, color2Bg);
            createLineChart('naspChart', 'NASP (USD)', naspData, color3, color3Bg);
            createLineChart('dcChart', 'DC (%)', dcData, color4, color4Bg);
            createLineChart('cvrChart', 'CVR (%)', cvrData, meliYellow, meliYellowBg);
            
            createAgg1Chart('agg1NmvChart', agg1Nmv, 'NMV (M USD)', meliYellow, meliYellowBg);
            createAgg1Chart('agg1NsiChart', agg1Nsi, 'NSI (K)', color2, color2Bg);
            createAgg1Chart('agg1NaspChart', agg1Nasp, 'NASP (USD)', color3, color3Bg); 
            
            createDonutChart('bpcChart', ['PM100', 'Smart', 'Seller'], [55, 20, 3], [meliYellow, color2, color3]);

            const aspLabels = ['0‚Äì10', '10‚Äì15', '15‚Äì20', '20‚Äì25', '25‚Äì50', '50‚Äì75', '+75 USD'];
            
            // Placeholder data for the 5 new charts
            const aspNmvData = [80, 70, 60, 50, 40, 30, 20]; // M USD
            const aspNsiData = [12000, 8000, 4000, 2000, 1000, 500, 200]; // K Units
            const aspDcData = [5.0, 5.5, 6.0, 6.5, 6.2, 5.8, 4.5]; // DC %
            const aspCvrData = [2.5, 2.3, 2.0, 1.8, 1.5, 1.2, 1.0]; // CVR %
            const aspBpcNewData = [95, 90, 85, 80, 75, 65, 55]; // BPC %
            
            // New chart calls for ASP Ranges 
            createHorizontalBarChart('aspNmvChart', aspLabels, aspNmvData, 'NMV (M USD)', meliYellow, meliYellowBg);
            createHorizontalBarChart('aspNsiChart', aspLabels, aspNsiData, 'NSI (K)', color2, color2Bg);
            createHorizontalBarChart('aspDcChart', aspLabels, aspDcData, 'DC (%)', color3, color3Bg);
            createHorizontalBarChart('aspCvrChart', aspLabels, aspCvrData, 'CVR (%)', color4, color4Bg);
            createHorizontalBarChart('aspBpcNewChart', aspLabels, aspBpcNewData, 'BPC (%)', meliYellow, meliYellowBg);
            
            // New Combo Charts for Site Performance (Now showing 6M history)
            createSiteHistoryChart('siteMlbChart', 'MLB', siteData.MLB);
            createSiteHistoryChart('siteMlmChart', 'MLM', siteData.MLM);
            createSiteHistoryChart('siteMlaChart', 'MLA', siteData.MLA);
            createSiteHistoryChart('siteMlcChart', 'MLC', siteData.MLC);
            createSiteHistoryChart('siteMcoChart', 'MCO', siteData.MCO);
            createSiteHistoryChart('siteMluChart', 'MLU', siteData.MLU);
            createSiteHistoryChart('siteMpeChart', 'MPE', siteData.MPE);
            createSiteHistoryChart('siteMecChart', 'MEC', siteData.MEC);

      });
